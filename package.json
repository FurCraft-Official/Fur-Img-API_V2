{
  "name": "random-image-api",
  "version": "2.2.0",
  "description": "Random Image API with Redis caching, multi-threading, CORS support and intelligent cache management",
  "main": "src/app.js",
  "scripts": {
    "init": "node src/init.js",
    "start": "npm run init && node src/app.js",
    "dev": "npm run init && set NODE_ENV=development && node src/app.js",
    "prod": "npm run init && set NODE_ENV=production && node src/app.js",
    "clean": "del /q *.json.lock *.json.tmp list.json.backup 2>NUL"
  },
  "keywords": [
    "image-api",
    "random",
    "redis",
    "cache",
    "multi-thread",
    "cors"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "hono": "^4.8.12",
    "https": "^1.0.0",
    "lru-cache": "^11.1.0",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "path": "^0.12.7",
    "redis": "^4.6.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "114514-lang",
  "license": "GPL-3",
  "devDependencies": {
    "jest": "^30.2.0"
  }
}
